{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Example: All",
      "program": "${workspaceRoot}/src/examples/all.ts",
      "outFiles": [
        "${workspaceRoot}/build/main/**/*.js"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js",
        "${workspaceRoot}/node_modules/**/*.js",
        "$!{workspaceRoot}/node_modules/bcoin/**/*.js"
      ],
      "stopOnEntry": true,
      "smartStep": true
    }
  ]
}
